{
    "version": "2.0.0",
    "command": "cmd",
    "type": "shell",
    "args": ["/C"],
    "tasks": [
        {
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "label": "make",
            "command": "${env:GDK}\\bin\\make",
            "args": [
                "-f",
                "${env:GDK}\\makefile.gen",
                "-j",
                "50"
            ],
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "shared",
                "showReuseMessage": false,
                "clear": true
            },
            "problemMatcher": []
        },
        {
            "label": "clean",
            "command": "${env:GDK}\\bin\\make",
            "args": [
                "clean",
                "-f",
                "${env:GDK}\\makefile.gen"
            ],
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "shared",
                "showReuseMessage": false,
                "clear": true
            },
            "problemMatcher": []
        },
        {
            "label": "run-emulator",
            "command": "d:\\bin\\blast.bat",
            "args": [
            ],
            //"dependsOn": "make",
            "type": "shell",
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "shared",
                "showReuseMessage": false,
                "clear": false
            },
            "problemMatcher": []
        }
    ]
}
